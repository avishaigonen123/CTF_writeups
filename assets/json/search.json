---
layout: none
---
[
  {% assign pages = site.pages | where_exp: "p", "p.title" %}
  {% for post in pages %}
    {
      "title": "{{ post.title | escape }}",
      "url": "{{ post.url | relative_url }}",
      "content": {{ post.content | strip_html | strip_newlines | jsonify }}
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]